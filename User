package Project2;

import java.io.Serializable;
import java.util.*;

public class User implements Serializable {
	protected String num; // 회원번호
	protected String name; // 회원이름
	protected String birth; // 생년월일
	protected String gender; // 성별
	protected String phone; // 핸드폰번호
	protected String job; // 직업
	protected String address; // 주소
	protected String kind; // 서비스종류
	protected String start_day; // 등록일
	protected String end_day; // 만료일
	public User(String num, String name, String birth, String gender, String phone, String job, String address, String kind, String start_day, String end_day)
	{
		this.num = num;
		this.name = name;
		this.birth = birth;
		this.gender = gender;
		this.phone = phone;
		this.job = job;
		this.address = address;
		this.kind = kind;
		this.start_day = start_day;
		this.end_day = end_day;
	} // 생성자

	public String getNum()
	{
		return this.num; 
	} // 회원번호  getter
	public String getName()
	{
		return this.name;
	} // 회원이름 getter
	public String getBirth()
	{
		return this.birth;
	} // 생년월일 getter
	public String getGender()
	{
		return this.gender;
	} // 성별 getter
	public String getPhone()
	{
		return this.phone;
	} // 핸드폰번호 getter
	public String getJob()
	{
		return this.job;
	} // 직업 getter
	public String getAddress()
	{
		return this.address;
	} // 주소 getter
	public String getKind()
	{
		return this.kind;
	} // PT종류 getter

	public String getStart_day()
	{
		return this.start_day;
	} // 등록일 getter
	public String getEnd_day()
	{
		return this.end_day;
	} // 만료일 getter
	public void setNum(String num)
	{
		this.num = num;
	} // 회원번호 setter
	public void setName(String name)
	{
		this.name = name;
	} // 회원이름 setter
	public void setBirth(String birth)
	{
		this.birth = birth;
	} // 생년월일 setter
	public void setGender(String gender)
	{
		this.gender = gender;
	} // 성별 setter
	public void setPhone(String phone)
	{
		this.phone = phone;
	} // 핸드폰번호 setter
	public void setJob(String job)
	{
		this.job = job;
	} // 직업 setter
	public void setAddress(String address)
	{
		this.address = address;
	} // 주소 setter
	public void setKind(String kind)
	{
		this.kind = kind;
	} // 서비스종류 setter

	public void setStart_day(String start_day)
	{
		this.start_day = start_day;
	} // 등록일 setter
	public void setEnd_day(String end_day)
	{
		this.end_day = end_day;
	} // 만료일 setter
	public Vector setVector()
	{
		Vector v = new Vector();
		v.add(this.num);
		v.add(this.name);
		v.add(this.birth);
		v.add(this.gender);
		v.add(this.phone);
		v.add(this.job);
		v.add(this.address);
		v.add(this.kind);
		v.add(this.start_day);
		v.add(this.end_day);
		return v;		
	} // User의 객체 벡터화
}
